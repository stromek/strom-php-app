{
    "name": "stromek/strom-php-app",
    "type": "project",
    "require": {
        "bref/bref": "^2.3",
        "php": "^8.2",
        "tracy/tracy": "^2.10",
        "php-di/php-di": "^7.0",
        "guzzlehttp/psr7": "^2.7",
        "dibi/dibi": "^5.0",
        "vlucas/phpdotenv": "^5.6",
        "zircote/swagger-php": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5",
        "phpstan/phpstan": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "authors": [
        {
            "name": "Petr Bíža",
            "email": "petr.biza@gmail.com"
        }
    ]
}
